{
  "openapi": "3.0.1",
  "info": {
    "title": "Authorize",
    "version": "1.0"
  },
  "paths": {
    "/api/Authorize/Login": {
      "post": {
        "tags": [
          "Authorize"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUser"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUser"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUser"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Authorize/Register": {
      "post": {
        "tags": [
          "Authorize"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUser"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUser"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUser"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
